{
      "arrowParens": "always",
      "bracketSpacing": false,
      "printWidth": 120,
      "semi": true,
      "singleQuote": true,
      "tabWidth": 4,
      "trailingComma": "all",
      "useTabs": false,
      "parser": "typescript",
      "proseWrap": "preserve",
      "overrides": [
            {
                  "files": ".js",
                  "options": {
                        "arrowParens": "always",
                        "bracketSpacing": false,
                        "printWidth": 120,
                        "semi": true,
                        "singleQuote": true,
                        "tabWidth": 4,
                        "trailingComma": "all",
                        "useTabs": false,
                        "parser": "babel",
                        "proseWrap": "preserve"
                  }
            },
            {
                  "files": "*.html",
                  "options": {
                        "parser": "angular",
                        "printWidth": 230,
                        "singleAttributePerLine": false,
                        "bracketSpacing": false,
                        "bracketSameLine": true
                  }
            },
            {
                  "files": ["*.json", "./.prettierrc"],
                  "options": {
                        "parser": "json",
                        "printWidth": 200,
                        "tabWidth": 6
                  }
            }
      ]
}
